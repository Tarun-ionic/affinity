{
  "name": "affinity",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "which vue-cli-service",
    "build:widget": "vue-cli-service build --target wc --inline-vue --name affinity-chat ./src/components/config/Widget.vue",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --url http://localhost:8080/",
    "lint": "vue-cli-service lint",
    "ci:run-parse": "cd backend/cloud/ && npm install && cd ../.. && docker-compose up -d && docker-compose logs --tail 10",
    "ci:setup-db": "npm run dev:db && npm run dev:db:mock-data && cp .env.development.local.template .env.development.local",
    "dev:db": "node -r dotenv/config bin/update-db.js dotenv_config_path=.env.development",
    "dev:db:destroy": "docker-compose stop && docker-compose rm -f",
    "dev:db:reset": "npm run dev:db:destroy && docker-compose up -d && npm run dev:db && npm run dev:db:mock-data && cp .env.development.local.template .env.development.local",
    "dev:db:mock-data": "node -r dotenv/config bin/mock-data.js dotenv_config_path=.env.development with-faq with-posts",
    "dev:db:add-posts": "node -r dotenv/config bin/mock-data.js dotenv_config_path=.env.development with-posts",
    "dev:db:add-faqs": "node -r dotenv/config bin/mock-data.js dotenv_config_path=.env.development with-faq",
    "dev:run-parse": "docker-compose pull && docker-compose up -d && docker-compose logs -f --tail 10 parse",
    "dev:restart-parse": "cd backend/cloud/  && npm install && cd ../.. && docker-compose stop parse && docker-compose up -d && docker-compose logs -f --tail 10 parse",
    "deploy:db": "node -r dotenv/config bin/update-db.js dotenv_config_path=.env.production.local",
    "deploy:web": "ionic build --engine browser && cd backend/cloud && npm install && cd ../ && b4a deploy",
    "deploy:b4a": "npm run deploy:web && npm run deploy:web"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.3",
    "@capacitor/core": "2.4.3",
    "@capacitor/ios": "^2.4.3",
    "@ionic/vue": "^5.5.2",
    "@ionic/vue-router": "^5.5.2",
    "@types/markdown-it": "^12.0.1",
    "@types/parse": "2.18.2",
    "@types/pell": "^1.0.0",
    "@types/turndown": "^5.0.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.7",
    "easyqrcodejs": "^4.1.0",
    "get-urls": "^10.0.0",
    "ionicons": "^5.2.3",
    "lodash": "^4.17.20",
    "markdown-it": "^12.0.4",
    "parse": "^2.19.0",
    "turndown": "^7.0.0",
    "vue": "^3.0.4",
    "vue-router": "^4.0.1",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.12.1",
    "@capacitor/cli": "2.4.3",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-e2e-cypress": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-plugin-router": "~4.5.9",
    "@vue/cli-plugin-typescript": "~4.5.9",
    "@vue/cli-plugin-unit-jest": "~4.5.9",
    "@vue/cli-service": "4.5.9",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "cypress": "^6.3.0",
    "cypress-localstorage-commands": "^1.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "ts-node": "^9.1.1",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-alpha.8",
    "webpack-chain": "^6.5.1"
  },
  "description": "Affinity Progressive Communities Network App"
}
